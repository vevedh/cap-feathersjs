{
  "name": "cap-feathersjs",
  "version": "1.0.8",
  "description": "Capacitor Fethersjs Electron Integration",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "build-electron": "rimraf ./electron/dist && cd ./electron && tsc && rollup --config rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Herv√© de CHAVIGNY",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest",
    "fs-jetpack": "^2.4.0"
  },
  "devDependencies": {
    "@capacitor/android": "latest",
    "@capacitor/ios": "latest",
    "@types/node": "^12.0.0",
    "electron": "^15.5.5",
    "rimraf": "^3.0.0",
    "rollup": "^2.19.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tslib": "^1.13.0",
    "typescript": "^3.2.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "electron/",
    "server/",
    "CapFeathersjs.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    },
    "electron": {
      "src": "electron/dist/plugin.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vevedh/cap-feathersjs.git"
  },
  "bugs": {
    "url": "https://github.com/vevedh/cap-feathersjs.git/issues"
  }
}
